SERVICE_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
REDIS_URL=redis://localhost:6379
SERVER_DOMAIN=nsl.sh

# TTL for route entries in seconds (default: 600 = 10 minutes)
# Routes expire if not refreshed within this time.
# Agent refreshes every 300s, so 600s provides buffer for missed heartbeats.
ROUTES_TTL_SECONDS=600